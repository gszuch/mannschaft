
<table class="table table-hover">
    <thead>
        <tr>
            <th>Type</th>
            <th>Title</th>
            <th class="last-modified">Last Modified</th>
            <th>Branch/Download</th>
        </tr>
    </thead>
    <tbody>
        {{#if docs}}
        {{#each docs}}
        {{#equals branchID 0}}
        <tr>
            <td><i class="fa fa-file-text" aria-hidden="true"></i></td>
            <td><a href="document/{{ id }}">{{ title }}</a></td>
            <td class="last-modified">{{ date }}</td>
            <td><a href="/branch-type?id={{ id }}"><i class="fa fa-code-fork" aria-hidden="true"></i></a> / <a href="file/{{id}}"><i class="fa fa-download"
                    aria-hidden="true"></i></a></td>
        </tr>
        {{/equals}}
        {{/each}}
        {{else}}
            <tr>
                <td colspan="4">Please "Upload Files" to Begin...</td>
            </tr>
        {{/if}}
    </tbody>
</table>
