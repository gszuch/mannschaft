<h3>Contents</h3>
<pre style="font-family: arial;">{{ contents }}</pre>

<h3>Versions</h3>
<br>
<table class="table table-hover table-bordered">
    <thead>
        <th>Date</th>
        <th>Description</th>
        <th>Author</th>
        <th>Actions</th>
    </thead>
    <tbody>
        {{#if versions}}
        {{#each versions}}
            <tr>
                <td> {{ date }} </td>
                <td> {{ description }} </td>
                <td> {{ author }} </td>
                <td> <a href="../file/{{id}}"><i class="fa fa-download"
                    aria-hidden="true"></i></a> </td>
            </tr>
        {{/each}}
        {{else}}
            <tr>
                <td colspan="4">No versions</td>
            </tr>
        {{/if}}
    </tbody>
</table>
